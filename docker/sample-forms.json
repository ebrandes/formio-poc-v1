  {
    "title": "User Registration",
    "name": "registrationForm",
    "path": "registration",
    "type": "form",
    "display": "wizard",
    "tags": ["registration"],
    "components": [
      {
        "title": "Account Information",
        "label": "Page 1",
        "type": "panel",
        "key": "page1",
        "components": [
          {
            "label": "Username",
            "tableView": true,
            "validate": {
              "required": true,
              "minLength": 3,
              "maxLength": 20,
              "pattern": "^[a-zA-Z0-9_]+$",
              "customMessage": "Username must contain only letters, numbers, and underscores"
            },
            "key": "username",
            "type": "textfield",
            "input": true,
            "placeholder": "Choose a username"
          },
          {
            "label": "Email Address",
            "tableView": true,
            "validate": {
              "required": true
            },
            "key": "email",
            "type": "email",
            "input": true,
            "placeholder": "your.email@example.com"
          },
          {
            "label": "Password",
            "tableView": false,
            "validate": {
              "required": true,
              "minLength": 8,
              "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]{8,}$",
              "customMessage": "Password must contain at least 8 characters, one uppercase letter, one lowercase letter, one number and one special character"
            },
            "key": "password",
            "type": "password",
            "input": true,
            "protected": true,
            "placeholder": "Enter a strong password"
          },
          {
            "label": "Confirm Password",
            "tableView": false,
            "validate": {
              "required": true,
              "custom": "valid = (input === data.password) ? true : 'Passwords must match';"
            },
            "key": "confirmPassword",
            "type": "password",
            "input": true,
            "protected": true,
            "placeholder": "Re-enter your password"
          }
        ]
      },
