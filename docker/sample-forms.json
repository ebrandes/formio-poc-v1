  {
    "title": "Contact Form",
    "name": "contactForm",
    "path": "contact",
    "type": "form",
    "display": "form",
    "tags": ["common"],
    "components": [
      {
        "label": "First Name",
        "tableView": true,
        "validate": {
          "required": true,
          "minLength": 2,
          "maxLength": 50
        },
        "key": "firstName",
        "type": "textfield",
        "input": true,
        "placeholder": "Enter your first name"
      },
      {
        "label": "Last Name",
        "tableView": true,
        "validate": {
          "required": true,
          "minLength": 2,
          "maxLength": 50
        },
        "key": "lastName",
        "type": "textfield",
        "input": true,
        "placeholder": "Enter your last name"
      },
      {
        "label": "Email",
        "tableView": true,
        "validate": {
          "required": true,
          "pattern": "[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,}$",
          "customMessage": "Please enter a valid email address"
        },
        "key": "email",
        "type": "email",
        "input": true,
        "placeholder": "your.email@example.com"
      },
      {
        "label": "Phone Number",
        "tableView": true,
        "validate": {
          "pattern": "^[\\+]?[(]?[0-9]{3}[)]?[-\\s\\.]?[0-9]{3}[-\\s\\.]?[0-9]{4,6}$",
          "customMessage": "Please enter a valid phone number"
        },
        "key": "phoneNumber",
        "type": "phoneNumber",
        "input": true,
        "placeholder": "(123) 456-7890"
      },
      {
        "label": "Subject",
        "widget": "choicesjs",
        "tableView": true,
        "data": {
          "values": [
            {
              "label": "General Inquiry",
              "value": "general"
            },
            {
              "label": "Technical Support",
              "value": "support"
            },
            {
              "label": "Sales",
              "value": "sales"
            },
            {
              "label": "Partnership",
              "value": "partnership"
            }
          ]
        },
        "validate": {
          "required": true
        },
        "key": "subject",
        "type": "select",
        "input": true
      },
      {
        "label": "Message",
        "autoExpand": false,
        "tableView": true,
        "validate": {
          "required": true,
          "minLength": 10,
          "maxLength": 1000
        },
        "key": "message",
        "type": "textarea",
        "input": true,
        "rows": 5,
        "placeholder": "Enter your message here..."
      },
      {
        "label": "Subscribe to Newsletter",
        "tableView": false,
        "defaultValue": false,
        "key": "subscribe",
        "type": "checkbox",
        "input": true
      },
      {
        "label": "Submit",
        "showValidations": false,
        "theme": "primary",
        "tableView": false,
        "key": "submit",
        "type": "button",
        "input": true,
        "saveOnEnter": true
      }
    ]
  },
